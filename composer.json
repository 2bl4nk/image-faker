{
    "name": "andyftw/image-faker",
    "description": "Image provider for Faker",
    "homepage": "https://github.com/andyftw/image-faker",
    "version": "1.0.0",
    "keywords": ["faker", "fixtures"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andy",
            "email": "info@andyfront.de",
            "homepage": "https://andyfront.de"
        }
    ],
    "require": {
        "php": "^5.3.2 || ^7.0",
        "fzaninotto/faker": "~1.4",
        "Imanee/Imanee": "dev-master",
        "symfony/finder": "^2.2 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5 || ^5.0.5",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": { "Andyftw\\Faker\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Andyftw\\Faker\\Test\\": "tests" }
    }
}
